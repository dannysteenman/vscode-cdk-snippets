{
  "name": "cdk-snippets",
  "displayName": "CDK Construct Snippets",
  "description": "This extension adds L1 construct snippets from CDK into Visual Studio Code.",
  "author": "Danny Steenman",
  "version": "1.0.0",
  "publisher": "dsteenman",
  "icon": "images/cdk-extension-icon.png",
  "homepage": "https://dannys.cloud",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/dannysteenman/vscode-cdk-snippets"
  },
  "engines": {
